#!/home/torpedo/Workspace/mip/mip-env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'medical-image-processor','console_scripts','mip-batch'
__requires__ = 'medical-image-processor'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('medical-image-processor', 'console_scripts', 'mip-batch')()
    )
